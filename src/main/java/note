vm options

--add-exports javafx.base/com.sun.javafx.event=org.controlsfx.controls,javafx.fxml
--add-opens javafx.graphics/com.sun.javafx.text=ALL-UNNAMED
--add-exports javafx.graphics/com.sun.javafx.print=ALL-UNNAMED


run the jar
java --add-exports javafx.base/com.sun.javafx.event=ALL-UNNAMED --module-path %PATH_TO_FX% --add-modules javafx.controls,javafx.fxml,javafx.graphics,javafx.media,javafx.web -jar  PrescriptionCreator_jar.jar